Algoritmo porcentaje
	precioInicial <- 0
	precioFinal <- 0
	incremento <- 0
	porciento <- 0
	Escribir 'Dame el precio inicial: '
	Leer precioInicial
	Escribir 'Dame el precio final: '
	Leer precioFinal
	incremento <- precioFinal-precioInicial
	porciento <- (incremento*100)/precioInicial
	Escribir 'El porcentaje de incremento de ',precioInicial,' pesos y ',precioFinal,' pesos es ',porciento,'%'
FinAlgoritmo
